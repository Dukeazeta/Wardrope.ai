{
  "name": "wardrope_ai_backend",
  "version": "1.0.0",
  "description": "Backend API for Wardrope.ai - AI-powered virtual wardrobe management system",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "dev:ts": "ts-node src/index.ts",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate": "ts-node database/migration.ts migrate",
    "db:check": "ts-node database/migration.ts check",
    "db:seed": "ts-node database/migration.ts seed",
    "db:reset": "ts-node database/migration.ts reset",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": ["ai", "fashion", "wardrobe", "virtual-try-on", "image-processing", "supabase"],
  "author": "Wardrope.ai Team",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "multer": "^1.4.5-lts.1",
    "@google/generative-ai": "^0.21.0",
    "sharp": "^0.33.5",
    "uuid": "^9.0.1",
    "@supabase/supabase-js": "^2.39.0",
    "pg": "^8.11.3",
    "@aws-sdk/client-s3": "^3.658.1",
    "@aws-sdk/s3-request-presigner": "^3.658.1",
    "axios": "^1.7.7"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.4",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.12",
    "@types/node": "^24.9.1",
    "@types/uuid": "^9.0.8",
    "@types/pg": "^8.10.9",
    "nodemon": "^3.1.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
